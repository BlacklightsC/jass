{
  
    "comments": {
        "lineComment": "//",
        "blockComment": ["/*", "*/"]
    },
    "brackets": [
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""]
    ],
    "wordPattern": "",
    "surroundingPairs": [
        ["[", "]"],
        ["(", ")"]
    ],
    "indentationRules": {
        "increaseIndentPattern": "\\b(returns|then|loop|library|scope|elseif|else|globals|struct)\\b",
        "decreaseIndentPattern": "\\b(elseif|else|endfunction|endif|endloop|endlibrary|endscope|endglobals|endstruct|endmethod)\\b"
    }
}
