set udg_posCastingUnit=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoc(1,'n00Q',GetOwningPlayer(GetAttacker()),udg_posCastingUnit,bj_UNIT_FACING)
if((GetUnitTypeId(GetAttacker())=='n00P'))then
call UnitAddAbilityBJ('A00N',GetLastCreatedUnit())
else
if((GetUnitTypeId(GetAttacker())=='n00R'))then
call UnitAddAbilityBJ('A00R',GetLastCreatedUnit())
else
call UnitAddAbilityBJ('A00U',GetLastCreatedUnit())
endif
endif
call UnitApplyTimedLifeBJ(2.,'BTLF',GetLastCreatedUnit())
call IssueTargetOrder(GetLastCreatedUnit(),"chainlightning",GetAttackedUnitBJ())
call RemoveLocation(udg_posCastingUnit)
else
if((udg_TAB_RANDOM[GetUnitUserData(GetAttacker())]<=25))then
set udg_posCastingUnit=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoc(1,'n00Q',GetOwningPlayer(GetAttacker()),udg_posCastingUnit,bj_UNIT_FACING)
if((GetUnitTypeId(GetAttacker())=='n00P'))then
call UnitAddAbilityBJ('A00O',GetLastCreatedUnit())
else
if((GetUnitTypeId(GetAttacker())=='n00R'))then
call UnitAddAbilityBJ('A00S',GetLastCreatedUnit())
else
call UnitAddAbilityBJ('A00V',GetLastCreatedUnit())
endif
endif
call UnitApplyTimedLifeBJ(2.,'BTLF',GetLastCreatedUnit())
call IssueTargetOrder(GetLastCreatedUnit(),"innerfire",GroupPickRandomUnit(YDWEGetUnitsInRangeOfLocMatchingNull(1000.,udg_posCastingUnit,Condition(function Trig_Spell_tower_triggerFunc003Func001Func007003001003))))
call AddSpecialEffectTargetUnitBJ("overhead",GetAttacker(),"Abilities\\Spells\\Undead\\ReplenishHealth\\ReplenishHealthCasterOverhead.mdl")
call RemoveLocation(udg_posCastingUnit)
call DestroyEffect(GetLastCreatedEffectBJ())
else
if((udg_TAB_RANDOM[GetUnitUserData(GetAttacker())]<=30))then
if((GetUnitTypeId(GetAttacker())=='n00P'))then
call AdjustPlayerStateBJ((udg_LVL*2),GetOwningPlayer(GetAttacker()),PLAYER_STATE_RESOURCE_GOLD)
else
if((GetUnitTypeId(GetAttacker())=='n00R'))then
call AdjustPlayerStateBJ((udg_LVL*3),GetOwningPlayer(GetAttacker()),PLAYER_STATE_RESOURCE_GOLD)
else
call AdjustPlayerStateBJ((udg_LVL*4),GetOwningPlayer(GetAttacker()),PLAYER_STATE_RESOURCE_GOLD)
endif
endif
call AddSpecialEffectTargetUnitBJ("overhead",GetAttacker(),"Abilities\\Spells\\Items\\ResourceItems\\ResourceEffectTarget.mdl")
call DestroyEffect(GetLastCreatedEffectBJ())
else
if((GetUnitTypeId(GetAttacker())=='n00S')and(udg_TAB_RANDOM[GetUnitUserData(GetAttacker())]<=33))then
set udg_posCastingUnit=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoc(1,'n00Q',GetOwningPlayer(GetAttacker()),udg_posCastingUnit,bj_UNIT_FACING)
call UnitAddAbilityBJ('A00W',GetLastCreatedUnit())
call IssueTargetOrder(GetLastCreatedUnit(),"fingerofdeath",GetAttackedUnitBJ())
call UnitDamageTargetBJ(GetLastCreatedUnit(),GetAttackedUnitBJ(),(GetUnitStateSwap(UNIT_STATE_LIFE,GetAttackedUnitBJ())*.75),ATTACK_TYPE_MAGIC,DAMAGE_TYPE_DIVINE)
call UnitApplyTimedLifeBJ(2.,'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_posCastingUnit)
else
endif
endif
endif
endif
endfunction
