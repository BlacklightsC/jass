{
  "name": "jass",
  "displayName": "jass",
  "description": "目标是扩展成为主流的jass插件。\nQQ群：137199441，提出你宝贵的建议。",
  "publisher": "jass",
  "icon": "src/resources/static/jass.png",
  "version": "0.0.47",
  "keywords": [
    "jass",
    "warcraft"
  ],
  "engines": {
    "vscode": "^1.37.0"
  },
  "markdown": "github",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/naichabaobao/jass.git"
  },
  "homepage": "https://gitee.com/naichabaobao/jass/raw/master/README.md",
  "activationEvents": [
    "onLanguage:jass"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Jass",
      "properties": {
        "jass.common_j": {
          "type": "string",
          "description": "自定义common.j,不设置时默认使用插件提供的common.j(test)\n(Customize common.j, using the common.j provided by the plug-in by default when not set)"
        },
        "jass.includes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "包含路徑或文件(如：ydwe，japi等),插件會找到.j|.ai\n(Contains a path or file (such as ydwe, japi, etc.) that the plug-in will find. J |.ai)"
        },
        "jass.diagnostic.support": {
          "type": "boolean",
          "default": true,
          "description": "是否开始错误检查\n(Whether to start error checking (currently only support call checking))"
        },
        "jass.format.indent.support": {
          "type": "boolean",
          "default": true,
          "description": "是否开始缩进格式化（如果你文件包含非jass语法，插件并不能保证缩进可用）\n(Whether to start indenting formatting)"
        },
        "jass.format.indent.char": {
          "type": "string",
          "default": " ",
          "enum": [
            " ",
            "\t"
          ],
          "description": "缩进字符(deprecated)\n(indent char)"
        },
        "jass.format.if_space": {
          "type": "number",
          "default": 0,
          "minimum": 0,
          "maximum": 8,
          "description": "if与左括号之间空格数(deprecated)\n(Number of spaces between if and left parenthesis)"
        },
        "jass.format.not_space": {
          "type": "number",
          "default": 0,
          "minimum": 0,
          "maximum": 8,
          "description": "not与左括号之间空格数(deprecated)\n(Number of spaces between not and left parenthesis)"
        }
      }
    },
    "languages": [
      {
        "id": "jass",
        "aliases": [
          "jass",
          "jass2"
        ],
        "extensions": [
          ".j",
          ".ai"
        ],
        "configuration": "./src/jass-configuration.json"
      }
    ],
    "grammars": [
      {
        "scopeName": "source.jass",
        "language": "jass",
        "path": "./src/jass.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "jass",
        "path": "./src/snippets.json"
      }
    ],
    "documentFormattingProvider": "true",
    "productIconTheme": [
      {
        "id": "jass",
        "label": "jass",
        "path": "./src/jass-product-icon-theme.json"
      }
    ],
    "enableProposedApi": true
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.37.0",
    "tslint": "^5.20.0",
    "typescript": "^3.7.4",
    "vscode-test": "^1.2.2"
  },
  "bugs": {
    "url": "https://github.com/fnfufnfu/jass/issues"
  },
  "dependencies": {},
  "author": "家猫hoo",
  "license": "GPL"
}
