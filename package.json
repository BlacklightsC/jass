{
  "name": "jass",
  "displayName": "jass",
  "description": "目标是扩展成为主流的jass插件。\nQQ群：137199441，提出你宝贵的建议。",
  "publisher": "jass",
  "icon": "src/resources/static/jass.png",
  "version": "0.0.30",
  "keywords": [
    "jass",
    "warcraft"
  ],
  "engines": {
    "vscode": "^1.37.0"
  },
  "markdown": "github",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fnfufnfu/jass.git"
  },
  "homepage": "https://raw.githubusercontent.com/fnfufnfu/jass/master/README.md",
  "activationEvents": [
    "onLanguage:jass"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Jass",
      "properties": {
        "jass.includes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "包含路徑,插件會找到.j|.ai文件(不會深度遍歷)"
        },
        "jass.vjass.support": {
          "type": "boolean", 
          "default": true,
          "description": "是否开启library支持"
        }
      }
    },
    "languages": [
      {
        "id": "jass",
        "aliases": [
          "jass",
          "jass2"
        ],
        "extensions": [
          ".j",
          ".ai"
        ],
        "configuration": "./src/jass-configuration.json"
      }
    ],
    "grammars": [
      {
        "scopeName": "source.jass",
        "language": "jass",
        "path": "./src/jass.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "jass",
        "path": "./src/snippets.json"
      }
    ],
    "documentFormattingProvider": "true"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "testjass": "tsc ./src/main/jass.ts & node ./src/main/jass.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.37.0",
    "tslint": "^5.20.0",
    "typescript": "^3.7.4",
    "vscode-test": "^1.2.2"
  },
  "bugs": {
    "url": "https://github.com/fnfufnfu/jass/issues"
  },
  "dependencies": {},
  "author": "家猫hoo",
  "license": "ISC"
}
