{
	"name": "jass",
	"displayName": "jass",
	"description": "",
  "version": "0.0.1",
  "keywords": ["vscode", "jass", "war3", "魔兽争霸", "魔獸爭霸", "編輯器", "魔獸", "魔兽"],
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:jass"
  ],
  "icon": "/static/icon/logo.svg",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
    ],
    "languages": [{
      "id": "jass",
      "extensions": [ ".j" ],
      "aliases": [ "jass", "j", "jass2" ]
  }]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
